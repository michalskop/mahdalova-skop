{
  "name": "mahdalova-skop",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev-site": "turbo run dev --filter=dev-site"
  },
  "devDependencies": {
    "chokidar": "^4.0.1",
    "prettier": "^3.0.0",
    "turbo": "^1.11.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
